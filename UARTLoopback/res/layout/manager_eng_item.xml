<?xml version="1.0" encoding="UTF-8"?>
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:descendantFocusability="blocksDescendants"
	    >
	    
	    <TextView 
		    android:layout_width="80dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_seq"
	        android:textColor="@color/title"
	        android:paddingLeft="20dp"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="90dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_mileage"
	        android:textColor="@color/title"
	        android:paddingLeft="15dp"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="110dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_total"
	        android:textColor="@color/title"
	        android:gravity="center"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="120dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_length"
	        android:textColor="@color/title"
	        android:gravity="center"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="120dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_xing"
	        android:textColor="@color/title"
	        android:gravity="center"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="100dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_minpress"
	        android:textColor="@color/title"
	        android:paddingLeft="30dp"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="100dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_maxpress"
	        android:textColor="@color/title"
	        android:paddingLeft="30dp"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="80dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_mincap"
	        android:textColor="@color/title"
	        android:paddingLeft="20dp"
	        android:layout_marginTop="10dp"
	        />
	    <TextView 
		    android:layout_width="80dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_maxcap"
	        android:textColor="@color/title"
	        android:paddingLeft="20dp"
	        android:layout_marginTop="10dp"
	        />
	   <TextView 
		    android:layout_width="200dp"
		    android:layout_height="wrap_content"
	        android:textSize="18sp"
	        android:id="@+id/manager_param_date"
	        android:textColor="@color/title"
	        android:paddingLeft="20dp"
	        />
	    <Button 
		    android:layout_width="50dp"
		    android:layout_height="40dp"
	        android:textSize="20sp"
	        android:id="@+id/manager_param_more"
	        android:focusable="false"
	        android:clickable="true"
	        android:layout_marginTop="18dp"
	        />
	</LinearLayout>